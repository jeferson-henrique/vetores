#include <stdio.h>

int main(){

int datas[4];

printf("\n\t quantos anos voce tem ");
scanf("%d",& datas[0]);
printf("\n\t quantos meses voce tem ");
scanf("%d",& datas[1]);
printf("\n\t quantos dias voce tem ");
scanf("%d",& datas[2]);

datas[0] = datas[0] * 365;
datas[1] = datas[1] * 30;
datas[3] = datas[2] + datas[0] + datas[1];

printf("\n\tvoce ja viveu %d dias", datas[3]);

return 0;

}
